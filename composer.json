{
    "name": "dweik/crud-generator",
    "description": "it's a easy tool for laravel +5.1 to generate crud functions base database tables",
    "keywords": [
        "laravel", "crud", "generator", "generate",
        "bootstrap", "mysql", "postgresql", "postgres",
        "sqlite", "artisan", "commandline", "command-line",
        "command", "route"
    ],
    "version": "1.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "abdelqader osama",
            "email": "asd.abyd@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^5.0,<5.6",
        "illuminate/console": "^5.0,<5.6",
        "illuminate/filesystem": "^5.0,<5.6",
        "symfony/class-loader": "^2.3|^3.0"
    },
    "require-dev": {
        "illuminate/config": "^5.0,<5.6",
        "illuminate/view": "^5.0,<5.6"
    },
    "autoload": {
        "psr-4": {
            "Dweik\\CrudGenerator\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dweik\\CrudGenerator\\CrudGeneratorServiceProvider"
            ]
        }
    }
}
